<script>
  document.addEventListener('DOMContentLoaded', function() {
    var generateButton = document.getElementById('generatePassword');
    var passwordInput = document.getElementById('password');
    var confirmPasswordInput = document.getElementById('confirmPassword');
    var togglePasswordVisibilityButton = document.getElementById('togglePasswordVisibility');
    var toggleConfirmPasswordVisibilityButton = document.getElementById('toggleConfirmPasswordVisibility');

    generateButton.addEventListener('click', function() {
      var generatedPassword = generateStrongPassword();
      passwordInput.value = generatedPassword;
      confirmPasswordInput.value = generatedPassword;
    });

    togglePasswordVisibilityButton.addEventListener('click', function() {
      togglePasswordVisibility(passwordInput, togglePasswordVisibilityButton);
    });

    toggleConfirmPasswordVisibilityButton.addEventListener('click', function() {
      togglePasswordVisibility(confirmPasswordInput, toggleConfirmPasswordVisibilityButton);
    });
  });

  function togglePasswordVisibility(inputField, toggleButton) {
    var type = inputField.getAttribute('type') === 'password' ? 'text' : 'password';
    inputField.setAttribute('type', type);
    toggleButton.textContent = type === 'password' ? 'Show' : 'Hide';
  }

  function generateStrongPassword() {
    // Logic to generate a strong password
    // You can customize this according to your requirements
    var characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789!@#$%^&*()-_=+';
    var passwordLength = 10;
    var password = '';
    for (var i = 0; i < passwordLength; i++) {
      var randomIndex = Math.floor(Math.random() * characters.length);
      password += characters[randomIndex];
    }
    return password;
  }
</script>

